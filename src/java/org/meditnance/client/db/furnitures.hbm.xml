<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditnance.client.db.Furniture" table="furnitures"/>
  <id name="id" column="id" type="integer" />

  <property name="_clientId" column="client_id" type="integer" index="client_id" />
  <property name="type" column="type" type="integer" />
  <property name="verification" column="verification" type="integer" />
  <property name="brand" column="brand" type="string" />
  <property name="ref" column="reference" type="string" />
  <property name="lastIntervention" column="last_intervention" type="date" />

  <many-to-one name="client" foreign-key="furnitures_ibfk_1" class="Client" name="client" column="client_id" />
</hibernate-mapping>
