<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditnance.client.db.Piece" table="pieces"/>
  <id name="id" column="id" type="integer" />

  <property name="idIntervention" index="id_intervention" column="id_intervention" type="integer" />
  <property name="provider" column="provider" type="string" />
  <property name="reference" column="reference" type="string" />

  <many-to-one name="intervention" class="Intervention" foreign-key="pieces_ibfk_1" column="idIntervention" />
</hibernate-mapping>
