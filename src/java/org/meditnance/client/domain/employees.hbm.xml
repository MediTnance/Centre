<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.meditnance.client.domain">
  <class name="org.meditnance.client.domain.Employee" table="employees">
    <id column="id" name="id" type="integer" unsaved-value="null"/>
    <property column="birth_date" name="birthDate" type="date"/>
    <property column="role" name="role" type="integer"/>
    <property column="first_name" name="firstName" type="string"/>
    <property column="last_name" name="lastName" type="string"/>
    <property column="address" name="address" type="string"/>
    <property column="specialization" name="specialization" type="string"/>
  </class>
</hibernate-mapping>
