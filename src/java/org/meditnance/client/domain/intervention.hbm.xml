<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditnance.client.domain.Intervention" table="interventions">
    <id column="id" name="id" type="integer"/>
    <property column="" name="cost" type="integer"/>
    <property column="nature" name="nature" type="integer"/>
    <property column="intervention_begin" name="begin" type="date"/>
    <property column="intervention_end" name="end" type="date"/>
    <property column="annotations" name="annotations" type="string"/>
    <many-to-one class="Client" name="client"/>
    <many-to-one class="Employee" name="employee"/>
  </class>
</hibernate-mapping>
