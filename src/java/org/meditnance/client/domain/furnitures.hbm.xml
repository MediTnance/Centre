<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditnance.client.domain.Furniture" table="furnitures">
    <id column="id" name="id" type="integer"/>
    <property column="type" name="type" type="integer"/>
    <property column="verification" name="verification" type="integer"/>
    <property column="brand" name="brand" type="string"/>
    <property column="reference" name="ref" type="string"/>
    <property column="last_intervention" name="lastIntervention" type="date"/>
    <many-to-one class="org.meditnance.client.domain.Client" column="client_id" name="client"/>
  </class>
</hibernate-mapping>
