<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditnance.client.domain.Piece" table="pieces">
    <id column="id" name="id" type="integer"/>
    <property column="provider" name="provider" type="string"/>
    <property column="reference" name="reference" type="string"/>
    <many-to-one class="Intervention" name="intervention"/>
  </class>
</hibernate-mapping>
