<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditenance.centre.domain.Employee" table="employees">
    <id name="id" type="integer">
      <generator class="native"/>
    </id>
    <property name="birthDate" not-null="false" type="date"/>
    <property name="role" type="integer"/>
    <property name="firstName" type="string"/>
    <property name="lastName" type="string"/>
    <property name="address" not-null="false" type="string"/>
    <property name="specialization" not-null="false" type="string"/>
  </class>
</hibernate-mapping>
