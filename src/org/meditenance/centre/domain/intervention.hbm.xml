<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.meditenance.centre.domain.Intervention" table="interventions">
    <id name="id" type="integer">
      <generator class="native"/>
    </id>
    <property name="cost" type="integer"/>
    <property name="nature" type="integer"/>
    <property name="begin" type="date"/>
    <property name="end" not-null="false" type="date"/>
    <property name="annotations" type="string"/>

    <many-to-one class="org.meditenance.centre.domain.Client" name="client" cascade="all" />
    <many-to-one class="org.meditenance.centre.domain.Employee" name="employee" cascade="all" />
  </class>
</hibernate-mapping>
